{
  "compilerOptions": {
    "target": "es5",                       
    "module": "esnext",                    
    "baseUrl": "./",                       
    "moduleResolution": "node",            
    "paths": {
      "@/*": ["src/*"]                    
    },
    "lib": ["esnext", "dom"],             
    "strict": true,                       
    "esModuleInterop": true,              
    "skipLibCheck": true,                 
    "forceConsistentCasingInFileNames": true 
  },
  "include": [
    "src/**/*"   
  ],
  "exclude": [
    "node_modules",                       
    "dist",                               
    "**/ParentPopup.vue"       
  ]
}
